export ELASTIFLOW_NETFLOW_LASTSW_TIMESTAMP=false
export ELASTIFLOW_NETFLOW_TZ=UTC

export ELASTIFLOW_SFLOW_IPV4_HOST=0.0.0.0
export ELASTIFLOW_SFLOW_IPV4_PORT=6343

export ELASTIFLOW_SFLOW_IPV6_HOST=[::]
